{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        { "type": "vcs", "url": "http://groupemsi.repositoryhosting.com/git_public/groupemsi/msiadminbundle.git" },
        { "type": "vcs", "url": "http://groupemsi.repositoryhosting.com/git_public/groupemsi/msiuserbundle.git" },
        { "type": "vcs", "url": "http://groupemsi.repositoryhosting.com/git_public/groupemsi/msimenubundle.git" },
        { "type": "vcs", "url": "http://groupemsi.repositoryhosting.com/git_public/groupemsi/msiblockbundle.git" },
        { "type": "vcs", "url": "http://groupemsi.repositoryhosting.com/git_public/groupemsi/msipagebundle.git" },
        { "type": "vcs", "url": "http://groupemsi.repositoryhosting.com/git_public/groupemsi/msipaginatorbundle.git" },
        { "type": "vcs", "url": "http://groupemsi.repositoryhosting.com/git_public/groupemsi/msisliderbundle.git" }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.0.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/swiftmailer-bundle": "2.1.*",
        "symfony/monolog-bundle": "2.1.*",
        "sensio/distribution-bundle": "2.1.*",
        "sensio/framework-extra-bundle": "2.1.*",
        "sensio/generator-bundle": "2.1.*",
        "jms/security-extra-bundle": "1.2.*",
        "jms/di-extra-bundle": "1.1.*",

        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "gedmo/doctrine-extensions": "dev-master",
        "friendsofsymfony/user-bundle": "dev-master",
        "knplabs/knp-menu-bundle": "dev-master",

        "msi/admin-bundle": "dev-master",
        "msi/user-bundle": "dev-master",
        "msi/menu-bundle": "dev-master",
        "msi/block-bundle": "dev-master",
        "msi/page-bundle": "dev-master",
        "msi/paginator-bundle": "dev-master",
        "msi/slider-bundle": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink"
    }
}
