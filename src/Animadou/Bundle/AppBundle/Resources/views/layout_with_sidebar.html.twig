{% extends 'AnimadouAppBundle::layout.html.twig' %}

{% block page_container %}
    <div class="row">
        <div class="span8">
            {{ msi_block_render('content_top', page) }}
            {{ block('page_content') }}
            {{ msi_block_render('content_bottom', page) }}
        </div>
        <div class="span4">
            {{ msi_block_render('sidebar_top', page) }}
            {{ msi_block_render('sidebar', page) }}
            {{ msi_block_render('sidebar_bottom', page) }}
        </div>
    </div>
{% endblock page_container %}
